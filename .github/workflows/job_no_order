name: job_no_order
on:
  workflow_dispatch:

jobs:
  job_1:
    runs-on: ubuntu-latest
    steps:
      - run: "sleep 30 ; echo Job ${{ github.job }}"

  job_2:
    runs-on: ubuntu-latest
    steps:
      - run: "echo Job ${{ github.job }}"

  job_3:
    runs-on: ubuntu-latest
    steps:
      - run: "echo Job ${{ github.job }}"
  
  job_4:
    runs-on: ubuntu-latest
    steps:
      - run: "echo Job ${{ github.job }}"
